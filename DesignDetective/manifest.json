{
    "manifest_version": 3,
    "name": "DesigDetective",
    "version": "1.0",
    "action": {
        "default_popup": "popup.html"
      },
    "permissions": ["activeTab", "scripting"],
    "content_scripts": [
      {
        "matches": ["*://*/*"],
        "js": ["content.js"]
      }
    ]
  }